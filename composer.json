{
    "name": "humbug/php-scoper",
    "description": "PHP-Scoper prefixes all PHP namespaces in a file/directory.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "webmozart/path-util": "^2.3",
        "symfony/filesystem": "^3.2",
        "symfony/process": "^3.2",
        "symfony/finder": "^3.2",
        "padraic/phar-updater": "^1.0.3",
        "nikic/php-parser": "^3.0",
        "mnapoli/silly": "^1.6",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "sebastian/version": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webmozart\\PhpScoper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmozart\\PhpScoper\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/php-scoper"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
